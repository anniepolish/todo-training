<% provide(:title, 'Add Profile Picture') %>
<h1>Add Picture</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@pic) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :url %>
      <%= f.text_field :url, class: 'form-control' %>

      <%= f.submit "Add Image", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
